{
    "name": "andez/selective-fixtures-loader-bundle",
    "description": "Symfony bundle for selective loading a fixture  with his dependencies",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Andez\\SelectiveFixturesLoaderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andez\\SelectiveFixturesLoaderBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Antonio Hernandez",
            "email": "andezdev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "symfony/console": "^7.3",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "phpstan/phpstan": "^2.1",
        "doctrine/coding-standard": "^14.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
